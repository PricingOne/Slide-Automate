    "# FILTERING SLIDES THAT HAS Brand WOB% empty\n",
    "def filter_bracket_dict(bracket_dict, column_name, required_values):\n",
    "    keys_to_delete = []\n",
    "    for key, df in bracket_dict.items():\n",
    "        if column_name not in df.columns:\n",
    "            keys_to_delete.append(key)\n",
    "        else:\n",
    "            values_in_df = df[column_name].astype(str).unique().tolist()\n",
    "            if not all(val in values_in_df for val in required_values):\n",
    "                keys_to_delete.append(key)\n",
    "    for key in keys_to_delete:\n",
    "        del bracket_dict[key]"
