    "import math\n",
    "from collections import Counter\n",
    "\n",
    "def divide_by_4(n):\n",
    "    return max(1, math.ceil(n / 4))\n",
    "\n",
    "def group_and_sum(data):\n",
    "    \"\"\"\n",
    "    - Extract unique first parts (before '|') → for labels.\n",
    "    - Count second parts (after '|') → for duplication calculation (divide by 4, round up).\n",
    "    - Return: total duplication count and list of unique first parts.\n",
    "    \"\"\"\n",
    "    catnum = set()  # to store unique first parts\n",
    "    group_counts = Counter()  # to count occurrences of each second part\n",
    "\n",
    "    if isinstance(data, dict):\n",
    "        for key in data.keys():\n",
    "            key_parts = key.split(' | ')\n",
    "            first_part = key_parts[0].strip()\n",
    "            second_part = key_parts[1].strip() if len(key_parts) > 1 else first_part\n",
    "\n",
    "            catnum.add(first_part)\n",
    "            group_counts[second_part] += 1\n",
    "        # Apply divide_by_4 to each second_part group count, then sum all\n",
    "        total = sum(divide_by_4(count) for count in group_counts.values())\n",
    "    else:\n",
    "        total = 0\n",
    "        catnum = set()\n",
    "\n",
    "    return total, list(catnum)\n"
   ]
